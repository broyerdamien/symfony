{% extends 'base.html.twig' %}

{% block title %}All programs
{% endblock %}

{% block body %}
	<h1>Toutes les séries de la table program :
	</h1>
	{% for category in categorys %}


		<h2 class="card-title">{{ loop.index }}
			/
			{{ category.id }}</h2>
		<p class="card-text">
			category:
			{{ category.name }}<br>


			<a href="{{ path("category_show", { categoryName: category.name}) }}" class="btn btn-primary">Go somewhere</a>
		</p>


	{% else %}
		Aucune série trouvée.
	{% endfor %}

	<a href="{{ path('app_index') }}">
		Retour à l'accueil
	</a>
{% endblock %}
